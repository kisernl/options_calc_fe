    "types": ["cypress", "node"],
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": ".."
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ]
}
